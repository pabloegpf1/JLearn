				<div class="card-header">
					<h3>User Profile</h3>
				</div>
				<div role="main"><span id="maincontent"></span><h2></h2>
					<form autocomplete="off" action="" method="post" accept-charset="utf-8" id="mform1" class="mform">
						<fieldset class="clearfix collapsible" id="id_moodle">
							<div class="fcontainer clearfix">
								<div id="firstname" class="form-group row  fitem  ">
									<div class="col-md-3">
										<label class="col-form-label d-inline " for="id_firstname">
											First name 
										</label>
									</div>
									<div class="col-md-9 form-inline felement" data-fieldtype="text">
										<input type="text" class="form-control " name="firstname" readonly="" disabled="" id="id_firstname" value="Izaac" size="30" maxlength="100" readonly>
									</div>
								</div>
								<div id="lastname" class="form-group row  fitem  ">
									<div class="col-md-3">
										<span class="pull-xs-right text-nowrap">
										</span>
										<label class="col-form-label d-inline " for="id_lastname">
											Last name
										</label>
									</div>
									<div class="col-md-9 form-inline felement" data-fieldtype="text">
										<input type="text" class="form-control " name="lastname" readonly="" disabled="" id="id_lastname" value="Garcilazo" size="30" maxlength="100" readonly>
									</div>
								</div>
								<div id="email" class="form-group row  fitem  ">
									<div class="col-md-3">
										<span class="pull-xs-right text-nowrap">
										</span>
										<label class="col-form-label d-inline " for="id_email">
											Email address
										</label>
									</div>
									<div class="col-md-9 form-inline felement" data-fieldtype="text">
										<input type="text" class="form-control " name="email" readonly="" disabled="" id="id_email" value="izaacg@mail.sfsu.edu" size="30" maxlength="100" readonly>
									</div>
								</div>
								<div id="studentId" class="form-group row  fitem  ">
									<div class="col-md-3">
										<span class="pull-xs-right text-nowrap">
										</span>
										<label class="col-form-label d-inline " for="id_studentId">
											Student Id
										</label>
									</div>
									<div class="col-md-9 form-inline felement" data-fieldtype="text">
										<input type="text" class="form-control " name="studentId" readonly="" disabled="" id="id_studentId" value="9000000001" size="30" maxlength="100" readonly>
									</div>
								</div>
							</div>
						</div>		 
					</div>

				</div>

				<div class="col-lg-3 col-sm-12 col-xs-12">				
					<% include ../partials/ilearn-support-card%>										
				</div>
